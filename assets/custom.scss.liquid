    }
  }
}
.image-with-text-row {
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  background-attachment: fixed;
  .image_with_text_wrap{
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-flex-direction: row;
    -ms-flex-direction: row;
    flex-direction: row;
    -webkit-justify-content: space-between;
    -ms-flex-pack: justify;
    justify-content: space-between;
    padding:5% 15px;
    .image_with_text_flex {
      width: 49%;
      background:#FFF;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
      flex-wrap: wrap;
      -webkit-flex-direction: row;
      -ms-flex-direction: row;
      flex-direction: row;
      -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
      justify-content: space-between;
      align-items:center;
      &.row-reverse{
        -webkit-flex-direction: row-reverse;
        -ms-flex-direction: row-reverse;
        flex-direction: row-reverse;
      }
      > div{
        width:50%;
        img{
          display:block; 
          width:100%;
        }
      }
      .text_block{
        text-align:center;
        h3{
          font-family: 'NOVA';
          font-weight: 400;
          letter-spacing: 0.1em;
        }
        p{
          font-size: 12px;
          font-weight: 600;
          line-height: 1.8em;
          padding: 0px 25px; 
        }
      }
    }
  }
}
.featured-blog-grid{
  background-size: cover;
  background-position: center;
  background-repeat: no-repeat;
  background-attachment: fixed;
  padding-top: 25px;
  .featured_blog_wrap{
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-flex-direction: row;
    -ms-flex-direction: row;
    flex-direction: row;
    -webkit-justify-content: space-between;
    -ms-flex-pack: justify;
    justify-content: space-between;
    padding: 3% 15px 0;
    .article__title > a:hover{text-decoration:none;}
    .blog_items{
      width: 50%; 
    }
    .blog_box{
      width:100%; 
      background:#FFF;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -webkit-flex-wrap: wrap;
      -ms-flex-wrap: wrap;
      flex-wrap: wrap;
      -webkit-flex-direction: row;
      -ms-flex-direction: row;
      flex-direction: row;
      -webkit-justify-content: space-between;
      -ms-flex-pack: justify;
      justify-content: space-between;
      margin-bottom:25px;
      align-items: center;
      &.big_box{
        align-items: start;
        height: calc(100% - 25px);
        margin-bottom: 0;
        > div{ 
          width:100%;
          &.blog_items {
            @media screen and (max-width: 1024px) {
              width:100%; 
              background:#FFF;
              display: -webkit-flex;
              display: -ms-flexbox;
              display: flex;
              -webkit-flex-wrap: wrap;
              -ms-flex-wrap: wrap;
              flex-wrap: wrap;
              -webkit-flex-direction: row;
              -ms-flex-direction: row;
              flex-direction: row;
              -webkit-justify-content: space-between;
              -ms-flex-pack: justify;
              justify-content: space-between;
              margin-bottom:25px;
              align-items: center;
              
              .image_block {
                width: 50%;
              }
              
              .text_block {
                width: 50%;
                padding: 0;
              }
            }
            
            @media screen and (max-width: 749px) {
              .image_block {
                width: 100%;
              }
              
              .text_block {
                width: 100%;
                padding: 5%;
                padding-bottom: 0;

                .article__title {
                  padding-left: 0;
                }
              }
            }
          }
        }
        .text_block{
          padding: 10%;
        }
      }
      &.row-reverse{
        -webkit-flex-direction: row-reverse;
        -ms-flex-direction: row-reverse;
        flex-direction: row-reverse;
      }
      > div{
        width:50%;
        img{
          display:block; 
          width:100%;
        }
      }
      .text_block{
        text-align:center;
        padding: 0 10px;
        .article__date{
          margin-bottom:0;
          font-size: 12px;
          color: #616060;
        }
        h3{
          font-family: 'NOVA';
          font-weight: 400;
          letter-spacing: 0.1em;
        }
        .article__grid-excerpt{
          font-size: 12px;
          font-weight: 600;
          line-height: 1.8em;
          padding: 0px 25px; 
        }
      }
    }
  }
}
