.btn-svg {
  position: relative;
  display: inline-block;
  width: 166px;
  height: 45px;
  line-height: 45px;
  text-align: center;
  text-transform: uppercase;
  color: #000;
  cursor: pointer;
  overflow: hidden;
}
.btn-svg svg {
  position: absolute;
  top: 0; left: 0;
}
.btn-svg svg rect {
  stroke: #000;
  stroke-width: 6;
  stroke-dasharray: 110, 312;
  stroke-dashoffset: 183;
 -webkit-transition: all 1200ms cubic-bezier(0.19, 1, 0.22, 1);
  -moz-transition: all 1200ms cubic-bezier(0.19, 1, 0.22, 1);
  transition: all 1200ms cubic-bezier(0.19, 1, 0.22, 1);
}
.btn-svg:hover svg rect {
  stroke-width: 6;
  stroke-dasharray: 422, 0;
  stroke-dashoffset: 0;
}
