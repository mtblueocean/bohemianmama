/*-=-=-=-=-=Custom-Fonts-=-=-=-=*/
@font-face {
  font-family: 'NOVA';
  src: url({{'NOVAegular.eot' | asset_url }});
  src: local('NOVA egular'), local('NOVAegular'),
    url({{'NOVAegular.eot' | asset_url }}?#iefix) format('embedded-opentype'),
    url({{'NOVAegular.woff2' | asset_url }}) format('woff2'),
    url({{'NOVAegular.woff' | asset_url }}) format('woff'),
    url({{'NOVAegular.ttf' | asset_url }}) format('truetype'),
    url({{'NOVAegular.svg' | asset_url }}#NOVAegular) format('svg');
  font-weight: normal;
  font-style: normal;
}

/*-=-=-=-=-=Custom-CSS-=-=-=-=*/
.full-width-wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 600px;
  background-size: cover;
  background-repeat: no-repeat;
  margin-top: -10px;
  margin-bottom: -10px;

  @media screen and (max-width: 484px) {
    margin-bottom: -30px;
  }

  .subscribe-form {
    width: 50%;

    @media screen and (max-width: 767px) {
      width: 80%;
    }
  }

  .subscribe-msg {
    padding: 0 5px;
    border-radius: 3px;
  }

  h1 {
    color: #FFF;
  }

  p {
    margin-bottom: 10px;
  }

  input {
    width: 100%;
    height: 50px;

    &:focus {
      outline: none;
      box-shadow: none;
      border: none;
    }
  }
}

.template-index{
  #AccessibleNav{
    &.border-bottom{
      border-bottom:0px;
    }
  }
}
.site-nav--centered{
  padding-bottom:0px; 
}
