<div class="gift_card_banner"> 
  <a href="{{ section.settings.link}}">
    <h1><img src="{{ section.settings.image | img_url: '2048x' }}" class="img-fluid" alt="Boho Holiday Free-Spirited Gifts"></h1>
   </a>
</div>

{% schema %}
  {
    "name": "Gift banner",
     
	"settings": [
 
 {
        "type": "image_picker",
        "id": "image",
        "label": "Image" 
      },
 
 
{
        "type": "url",
        "id": "link",
        "label": "link" 
      }
    ]
  }
{% endschema %}
